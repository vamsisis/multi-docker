{
    "AWSEBDockerrunVersion" : 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "1813354/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "server",
            "image": "1813354/multi-server",
            "hostname": "api",
            "essential": false
       },
       {
           "name": "worker",
           "image": "1813354/multi-worker",
           "hostname": "worker", 
           "essential": false 
       },
       {
           "name": "nginx",
           "image": "1813354/multi-nginx",
           "hostname": "nginx",
           "essential": true,
           "portMappings": [
                {
                 "hostPort": 80,
                 "containerPort": 80
                }
            ],
           "links": ["client", "server"]
        }       
    ]
}